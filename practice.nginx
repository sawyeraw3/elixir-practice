server {
    listen 80;
    listen [::]:80;

    server_name hw02b.sawyerwarden.com;

    location / {
        #proxy_redirect off;
        #proxy_pass http://hw02b.sawyerwarden.com
        proxy_pass http://localhost:4790;
    }

    location /socket {
        #proxy_pass http://hw02b.sawyerwarden.com;
        proxy_pass http://localhost:4790;
        proxy_http_version 1.1;
        proxy_set_header Upgrade $http_upgrade;
        proxy_set_header Connection "upgrade";
    }
}
